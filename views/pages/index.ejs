<!DOCTYPE html>
<html lang="en">
	<!--Tag ejs para importar arquivos dentro de outros-->
	<%- include('../partials/head'); %>
	<body class="container">
		<%- include('../partials/header'); %>
		<main>
			<div class="jumbotron">
				<h1>Node EJS</h1>
				<p>Learning EJS</p>
				<p><%= subtitle %></p>
			</div>

			<ul>
				<% qualities.forEach((quality) => { %>
				<li><strong><%= quality.title%></strong><%= quality.message %></li>
				<% }) %>
			</ul>
		</main>
		<%- include('../partials/footer'); %>
	</body>
</html>
